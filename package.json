{
  "name": "@newkrok/nape-js",
  "version": "2.0.0",
  "description": "Nape Physics Engine - Modern TypeScript wrapper for 2D physics simulation",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "format": "prettier --write src/ tests/",
    "format:check": "prettier --check src/ tests/",
    "benchmark": "npm run build && node benchmarks/run.mjs",
    "build:docs": "npm run build && cp dist/index.js docs/nape-js.esm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NewKrok/nape-js"
  },
  "keywords": [
    "nape",
    "physics",
    "2d",
    "typescript",
    "physics-engine",
    "collision-detection",
    "rigid-body"
  ],
  "author": "Istvan Krisztian Somoracz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NewKrok/nape-js/issues"
  },
  "homepage": "https://github.com/NewKrok/nape-js#readme",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.56.1",
    "vitest": "^3.0.0"
  }
}
